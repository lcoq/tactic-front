{{reviews-filters
  users=model.users
  selectedUsers=selectedUsers

  clients=model.clients
  selectedClients=selectedClients

  projects=projects
  selectedProjects=selectedProjects

  since=since
  before=before

  updateSelectedUsers=(action "updateSelectedUsers")
  updateSelectedClients=(action "updateSelectedClients")
  updateSelectedProjects=(action "updateSelectedProjects")
}}
{{entries-by-client-and-project
  list=entriesByClientAndProject
  didDeleteEntry=(action "didDeleteEntry")
  didUpdateEntry=(action "didUpdateEntry")
  searchProjects=(action "searchProjects")
}}
